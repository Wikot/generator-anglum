{
  "name": "<%= p.projectName %>",
  "description": "<%= p.projectDescription %>",
  "keywords": ["framework", "laravel", "lumen", "angularjs", "bower"],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/lumen-framework": "5.1.*",
    "vlucas/phpdotenv": "~1.0",
    "facebook/php-sdk-v4": "4.0.*",
    "guzzlehttp/guzzle": "~5.0",
    "guzzlehttp/oauth-subscriber": "*",
    "fzaninotto/faker": "~1.0"
  },
  "require-dev": {
      "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
        "App\\": "backend/app/"
    },
    "classmap": [
        "backend/database/"
    ]
  },
  "autoload-dev": {
    "classmap": [
        "backend/tests/"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}

